<p id="notice"><%= notice %></p>

<h1>Quotes</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Employee</th>
      <th>Car</th>
      <th>Wholesale Price</th>
      <th>Loan</th>
      <th>Multiplier</th>
      <th>Total Price</th>
      <th>Sold</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.customer %></td>
        <td><%= quote.employee %></td>
        <td><%= quote.car %></td>
        <td><%= quote.wholesaleprice %></td>
        <td><%= quote.loan %></td>
        <td><%= quote.multiplier %></td>
        <td><%= quote.totalprice %></td>
        <td><%= quote.sold %></td>
        <td><%= link_to 'Show', quote %></td>
        <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
        <td><%= link_to 'Destroy', quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote', new_quote_path %>
